{
  "metadata": {
    "name": "Pipe Simulation Example",
    "description": "A simple simulation of a reservoir connected to a downstream gate via a pipe.",
    "version": "1.0"
  },
  "simulation_settings": {
    "duration": 600,
    "dt": 1.0
  },
  "components": [
    {
      "id": "res1",
      "type": "Reservoir",
      "initial_state": {
        "volume": 25000000,
        "water_level": 15.0
      },
      "params": {
        "surface_area": 1500000
      }
    },
    {
      "id": "pipe1",
      "type": "Pipe",
      "initial_state": {
        "outflow": 0
      },
      "params": {
        "length": 1000,
        "diameter": 1.5,
        "friction_factor": 0.02
      }
    },
    {
      "id": "g1",
      "type": "Gate",
      "initial_state": {
        "opening": 0.3
      },
      "params": {
        "width": 10,
        "max_opening": 1.0,
        "discharge_coefficient": 0.6
      }
    }
  ],
  "connections": [
    {
      "from": "res1",
      "to": "pipe1"
    },
    {
      "from": "pipe1",
      "to": "g1"
    }
  ]
}
